<h2>マイページ編集</h2>

<%= form_with model: @creator, url: creator_creators_path, method: :patch do |f| %>
  <table>
    <tr>
      <th>プロフィール画像</th>
      <td><%= f.file_field :profile_image %></td>
    </tr>
    <tr>
      <th>名前</th>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <th>プロフィール</th>
      <td><%= f.text_area :profile %></td>
    </tr>
    <tr>
      <th>郵便番号</th>
      <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
      <th>住所</th>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <th>電話番号</th>
      <td><%= f.text_field :telephone_number %></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= f.email_field :email %></td>
    </tr>
  </table>
  <div><%= f.submit "更新する" %></div>
<% end %>
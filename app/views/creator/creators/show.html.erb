<h2>マイページ</h2>

<table>
  <tr>
    <th>名前</th>
    <td><%= @creator.name %></td>
  </tr>
  <tr>
    <th>プロフィール</th>
    <td><%= @creator.profile %></td>
  </tr>
  <tr>
    <th>郵便番号</th>
    <td><%= @creator.postal_code %></td>
  </tr>
  <tr>
    <th>住所</th>
    <td><%= @creator.address %></td>
  </tr>
  <tr>
    <th>電話番号</th>
    <td><%= @creator.telephone_number %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= @creator.email %></td>
  </tr>
</table>
<div><%= link_to "編集する", edit_creator_creators_path %></div>
<div><%= link_to "退会する", creator_creators_out_path %></div>